{
  "name": "lifegame",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "biome ci src",
    "preview": "vite preview",
    "pages:dev": "wrangler pages dev --port 3000 --script-path src/worker.ts -- npm start",
    "pages:build-worker": "npm run build",
    "pages:build-client": "npm run build -- --mode client",
    "pages:deploy": "npm run pages:build-client && npm run pages:build-worker && wrangler pages deploy ./dist --project-name lifegame",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "@radix-ui/themes": "^2.0.3",
    "react": "^18.3.0-canary-a870b2d54-20240314",
    "react-dom": "^18.3.0-canary-a870b2d54-20240314"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@cloudflare/workers-types": "^4.20240312.0",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "typescript": "^5.2.2",
    "urlpattern-polyfill": "^10.0.0",
    "vite": "^5.1.4",
    "wrangler": "^3.31.0"
  },
  "overrides": {
    "@radix-ui/themes": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
